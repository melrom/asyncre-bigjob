 GB MD with umbrella restraint
 &cntrl
   irest = 1, ntx = 5,                     ! restart and read velocities
   nstlim = 1000,                          ! simulation length (step limit)
   dt = 0.001,                             ! integration time (ps)
   ntpr = 500, ntwx = 500,                 ! print freq of energy and coordinats
   ntwr = 0,                               ! print freq of restart (will always print at end)
   ioutfm = 1,                             ! flag for binary coordinates              
   ntc = 2, ntf = 2, tol = 1e-8,           ! SHAKE for all bonds to hydrogen
   ntt = 3, temp0 = 310.0, gamma_ln = 1.0, ! Langevin dynamics 
   igb = 2,                                ! OBC GB solvation 
   nmropt = 1                              ! flag to read "nmr restraints"
 /
 &wt type='DUMPFREQ', istep1 = 500, /      ! print frequency for restrained coordinates
 &wt type='END', /
 DISANG=DMP_US_@n@.RST
 DUMPAVE=DMP_US_@n@.TRACE
 LISTIN=POUT
 LISTOUT=POUT

