 GB MD with umbrella restraint
 &cntrl
   irest = 1, ntx = 5,                     ! restart and read velocities
   nstlim = 1000,                          ! simulation length (step limit)
   ntpr = 500, ntwx = 500,                 ! print freq of energy and coords
   ioutfm = 1,                             ! use binary trajectory files  
   ntc = 2, ntf = 2, tol = 1e-8,           ! SHAKE for all bonds to hydrogen
   ntt = 3, temp0 = 310.0, gamma_ln = 5.0, ! Langevin dynamics 
   ig = -1,                                ! use wallclock to seed random #'s
   igb = 2,                                ! OBC GB solvation 
   nmropt = 1                              ! flag to read "nmr restraints"
 /
 &wt type='DUMPFREQ', istep1 = 500, / ! print frequency of restraint coords
 &wt type='END', /
 DISANG=DMP_US.RST
 DUMPAVE=TRACE
 LISTIN=POUT
